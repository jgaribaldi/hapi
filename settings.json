      "upstreams": ["localhost:8001", "localhost:8002"]
    }
  ],
  "probes": [
    {
      "upstream_address": "localhost:8001",
      "poll_interval_ms": 1000,
      "error_count": 5,
      "success_count": 5
    },
    {
      "upstream_address": "localhost:8002",
      "poll_interval_ms": 1000,
      "error_count": 5,
      "success_count": 5
    }
  ]
